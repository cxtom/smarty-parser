<INITIAL>{emptyjson} {
  return TP_TEXT;
}

<INITIAL>{ldel}"*"{text}*?"*"{rdel} {
  return TP_COMMENT;
}

<INITIAL>{comment} {
  var s = this._input, i, l, ch;
  debugger;
  for (i = 0, l = s.length; i < l; i++) {
    if (s[i] === '*' && s[i + 1] === this.rdel) {
      break;
    }
  }
  if (i >= l) {
    throw new Error('missing or misspelled comment closing tag \'*' + this.rdel + '\'');
  }
  this._consume(i);
  return TP_COMMENT;
}

