<INITIAL>{ldel}{literal}{rdel} {
  if (this.hasAutoLiteral()) {
    return TP_TEXT;
  } else {
    this.pushState('ST_LITERAL');
    return TP_LITERALSTART;
  }
}
<INITIAL>{ldel} {
  if (this.hasAutoLiteral()) {
    return TP_TEXT;
  } else {
    this.pushState('ST_TAG');
    this.unput(this.matched);
  }
}
<INITIAL>{rdel} {
  return TP_TEXT;
}
